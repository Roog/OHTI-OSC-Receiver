{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Kestrel": {
        "EndPoints": {
            "Http": {
                "Url": "http://+:5001"
            }
        },
        "Limits": {
            "MaxConcurrentConnections": 100,
            "MaxConcurrentUpgradedConnections": 100
        },
        "DisableStringReuse": true
    },
    "App": {
        "Receiver": {
            "Hostname": "255.255.255.255",
            "Port": 9000
        }
    }
}